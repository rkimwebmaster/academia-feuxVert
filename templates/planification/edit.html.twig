{% extends 'base.html.twig' %}

{% block title %}Edit Planification{% endblock %}
{% block titre %}
	Modifier une planification
{% endblock %}
{% block lesboutons %}
	<a href="{{ path('planification_index') }}" title="Retour à la liste " class="btn btn-info">
		<i class="fa fa-reply-all"></i>
	</a>
	<a type="button" href="{{url('fiche_new')}}" title="Ajouter nouveau " class="btn btn-info">
		<i class="fas fa-plus-circle"></i>
	</a>
	<a type="button" title="Modifier élément  " class="btn btn-info">
		<i class="fas fa-edit"></i>
	</a>
	<a type="button" title="Supprimer élément" class="btn btn-info">
		<i class="fas fa-trash"></i>
	</a>
{% endblock %}
{% block positionnement %}
	<span class="fa fa-angle-double-right"></span>
	Planification
	<span class="fa fa-angle-double-right"></span>
	Modification
{% endblock %}
{% block body %}
    {{ include('planification/_form.html.twig', {'button_label': 'Update'}) }}
    {{ include('planification/_delete_form.html.twig') }}
{% endblock %}
