{% block title %}Faculte
{% endblock %}
{% block lesboutons %}
	<a href="{{ path('faculte_index') }}" title="Retour à la liste " class="btn btn-info">
		<i class="fa fa-reply-all"></i>
	</a>
	<a href="{{ path('faculte_new') }}"  title="Ajouter nouveau " class="btn btn-info">
		<i class="fas fa-plus-circle"></i>
	</a>
	<a href="{{ path('faculte_edit', {'id': faculte.id}) }}" title="Modifier élément  " class="btn btn-info">
