			</tr>
			<tr>
				<th>Designation</th>
				<td>{{ faculte.designation }}</td>
			</tr>
			<tr>
				<th>Description</th>
				<td>{{ faculte.description }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('faculte_index') }}">back to list</a>

	<a href="{{ path('faculte_edit', {'id': faculte.id}) }}" class="btn btn-default btn-sm">
