{% extends 'base.html.twig' %}

{% block title %}New AnneeAcademique
{% endblock %}
{% block titre %}
	Créer nouvelle Annee Academique
{% endblock %}

{% block lesboutons %}
	<a title="Retour à la liste " class="btn btn-info" href='{{ path('annee_academique_index') }}'>
		<i class="fa fa-reply-all"></i>
	</a>
	<a type="button" title="Ajouter nouveau " class="btn btn-info">
		<i class="fas fa-plus-circle"></i>
	</a>
	<a type="button" title="Modifier élément  " class="btn btn-info">
		<i class="fas fa-edit"></i>
	</a>
	<a type="button" title="Supprimer élément" class="btn btn-info">
		<i class="fas fa-trash"></i>
	</a>
{% endblock %}
{% block body %}

	{{ form_start(form) }}
	{{ form_widget(form) }}


{% endblock %}
{% block cardfooter %}
	<button class="btn btn-info">
		<span class="fa fa-download "></span>

		{{ button_label|default('Enregistrer') }}</button>
	{{ form_end(form) }}

{% endblock %}
