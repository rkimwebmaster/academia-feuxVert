{% extends 'base.html.twig' %}

{% block title %}Finaliste
{% endblock %}
{% block lesboutons %}
	<a href="{{ path('finaliste_index') }}" title="Retour à la liste " class="btn btn-info">
		<i class="fa fa-reply-all"></i>
	</a>
	<a href="{{ path('finaliste_edit', {'id': finaliste.id}) }}" title="Modifier élément  " class="btn btn-info">
		<i class="fas fa-edit"></i>
	</a>
{% endblock %}
{% block titre %}
	Détail de finaliste
{% endblock %}
{% block body %}

	<table class="table">
		<tbody>
			<tr>
				<th>Noms </th>
				<td>{{ finaliste.identite.nom }}</td>
			</tr>
            <tr>
				<th>Noms </th>
				<td>{{ finaliste.identite.nom }}</td>
			</tr>
            <tr>
				<th>Télephone </th>
				<td>{{ finaliste.identite.telephone }}</td>
			</tr>
            <tr>
				<th>E-mail </th>
				<td>{{ finaliste.user.email }}</td>
			</tr>
		</tbody>
	</table>
	{% if is_granted('ROLE_ADMIN') %}
	{{ include('finaliste/_delete_form.html.twig') }}
	{% endif %}
{% endblock %}
