<form method="post" action="{{ path('finaliste_delete', {'id': finaliste.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ finaliste.id) }}">
    <button class="btn">Delete</button>
</form>
